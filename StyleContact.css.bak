.icon {
    display: inline-block;
    height: 60px;
    width: 60px;
    text-align: center;
    border-radius: 50px;
    box-sizing: border-box;
    line-height: 60px;
    transition: all 0.3s ease-out;
}

.b1:hover .icon {
    background: #0e76aB;
}

.b2:hover .icon {
    background: #E1306C;
}

.b3:hover .icon {
    background: #1DA1F2;
}

.b4:hover .icon {
    background: #4267B2;
}




.b1:hover .icon i {
    color: #fff;
}

.b2:hover .icon i {
    color: #fff;
}

.b3:hover .icon i {
