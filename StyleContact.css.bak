.b1, .b2, .b3, .b4 {
    display: inline-block;
    height: 60px;
    width: 60px;
    float: left;
    margin: 0 11.2px;
    overflow: hidden;
    background: #fff;
    border-radius: 50px;
    cursor: pointer;
    box-shadow: 0px 10px 10px rgb(6,36,9,.3);
    transition: all 0.3s ease-out;
}
