* {
    padding: 0;
    margin: 0;
    text-decoration: none;
    list-style: none;
    box-sizing: border-box;


}

body {
    font-family: Arial;
}

nav {
    background-color: white;
	box-shadow: 0px 1px 1px rgba(1,27,64,.1);
    height: 80px;
    width: 100%;
    position: fixed;
    z-index: 12;
}

label.logo {
    margin-left: 0;
}
.logoim {
    height: 3.3rem;
    width: 21rem;
    margin-top: .9rem;
    object-fit: fill;
	margin-left:.9rem;
    object-position: -30px 0;
}
#click{
    display:none;
}
nav ul {
    float: right;
    margin-right: 20px;
}

    nav ul li {
        display: inline-block;
        line-height: 80px;
        margin: 0 5px;
    }

        nav ul li a {
            color: black;
            font-size: 16px;
			  font-weight:bold;
            padding: 10px 15px;
            border-radius: 3px;
        }

a.active, a:hover {
    background: #0058f0;
	color: #ffffff;
    transition: .2s;
   
   
}
nav .menubtn{
    display:none;
}
.banner {
    width: 100%;
    margin-top: 0px;
    height: 400px;
    background-color:#e8f0ff;
    position: relative;
    background-size: cover;
    background-position: center center;
	
	
}


.banner h1 {
        padding-top: 22%;
        font-size: 50px;
        font-family: "Lucida Console", "Courier New", monospace;
        text-align: left;
		 font-weight: 500;
		 line-height:60px;
		 letter-spacing: 1px;
        text-transform: uppercase;
        /*text-shadow: 0 0 10px #000000;*/
        color: black;
		 padding-left: 19%;

 }
